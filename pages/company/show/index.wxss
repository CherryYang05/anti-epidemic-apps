
.date{
  display: block;
  font-size: 25rpx;
  color: #8f8f8f;
  margin-left: 1%;
  margin-right: 1%;
  margin-bottom: 40rpx;
  text-align: center;
  border: 1rpx dashed #999;
}
.table {
  border: 0px solid #f1fcff;
}
.scrollClass {
  display: flex;
  width: 200%;
  white-space: nowrap;
  margin-top: 23px;
  height: 100%;
  background-color: white;
}
